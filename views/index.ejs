      <tr>
        <th><b>ID#</b></th>
        <th><b>Term</b></th>
        <th><b>Description</b></th>
        <th><b>References</b></th>
      </tr>
      <% temps.forEach((temp) => { %>
        <tr>
          <td>
            <%= temp.id %>
          </td>
          <td>
            <%= temp.title %>
          </td>
          <td>
            <%= temp.des %>
          </td>
          <td>
            <%= temp.ref %>
          </td>
        </tr>
      <% }); %>
    </table>
